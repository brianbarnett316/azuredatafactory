{
	"name": "Egress Profisee REST API to CSV1",
	"properties": {
		"activities": [
			{
				"name": "REST API to CSV",
				"description": "Copy member data from Profisee via the Profisee REST API to ADLS Gen2 in CSV format.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"x-api-key": "4d506aeb27f540a0a042a65ebaa5ec81"
						},
						"paginationRules": {
							"AbsoluteUrl": "$.nextPage"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id",
									"type": "Guid"
								}
							},
							{
								"source": {
									"path": "['code']"
								},
								"sink": {
									"name": "code",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['name']"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['color']"
								},
								"sink": {
									"name": "color",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['class']"
								},
								"sink": {
									"name": "class",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['productSubCategory']"
								},
								"sink": {
									"name": "productSubCategory",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['sellEndDate']"
								},
								"sink": {
									"name": "sellEndDate",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['weight']"
								},
								"sink": {
									"name": "weight",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "['changedOn']"
								},
								"sink": {
									"name": "changedOn",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['changedByUser']"
								},
								"sink": {
									"name": "changedByUser",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "ProfiseeRESTAPI_Members_Get1",
						"type": "DatasetReference",
						"parameters": {
							"entityId": {
								"value": "@variables('EntityName')",
								"type": "Expression"
							},
							"pageSize": 100,
							"attributes": "Color,Class,ProductSubCategory,SellEndDate,Weight",
							"filter": " ",
							"orderBy": " ",
							"dbaFormat": 1
						}
					}
				],
				"outputs": [
					{
						"referenceName": "OutputBlobCsv1",
						"type": "DatasetReference",
						"parameters": {
							"Container": {
								"value": "@variables('OutputContainer')",
								"type": "Expression"
							},
							"Directory": {
								"value": "@variables('EntityName')",
								"type": "Expression"
							},
							"FileName": {
								"value": "@concat(variables('EntityName'), '_', string(utcnow()), '.csv')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"variables": {
			"OutputContainer": {
				"type": "String",
				"defaultValue": "profisee-output"
			},
			"EntityName": {
				"type": "String",
				"defaultValue": "product"
			}
		},
		"annotations": [],
		"lastPublishTime": "2020-09-25T15:29:08Z"
	}
}