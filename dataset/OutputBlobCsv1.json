{
	"name": "OutputBlobCsv1",
	"properties": {
		"description": "Azure Data Lake Storage Gen2 container for CSV format.",
		"linkedServiceName": {
			"referenceName": "BlobStorageGen2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Container": {
				"type": "string"
			},
			"Directory": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().Directory",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().Container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}