									},
									"FileName": {
										"value": "@concat(variables('EntityId'), '_', string(utcnow()), '.csv')",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"variables": {
					"OutputContainer": {
						"type": "String",
						"defaultValue": "profisee-output"
					},
					"EntityId": {
						"type": "String",
						"defaultValue": "product"
					}
				},
				"folder": {
					"name": "CSV Format"
				},
				"annotations": [],
