					"NewCsvMemberFile_properties_Ingress Staging Area CSV to REST API_parameters_FileFolder": {
						"value": "[parameters('NewCsvMemberFile_properties_Ingress Staging Area CSV to REST API_parameters_FileFolder')]"
					},
					"NewCsvMemberFile_properties_typeProperties_scope": {
						"value": "[parameters('NewCsvMemberFile_properties_typeProperties_scope')]"
					},
					"NewJsonMemberFile_properties_Ingress Staging Area JSON to REST API_parameters_FileName": {
						"value": "[parameters('NewJsonMemberFile_properties_Ingress Staging Area JSON to REST API_parameters_FileName')]"
					},
					"NewJsonMemberFile_properties_Ingress Staging Area JSON to REST API_parameters_FileFolder": {
						"value": "[parameters('NewJsonMemberFile_properties_Ingress Staging Area JSON to REST API_parameters_FileFolder')]"
					},
					"NewJsonMemberFile_properties_typeProperties_scope": {
						"value": "[parameters('NewJsonMemberFile_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'BrianBa-ADF_ArmTemplate_0')]"
			]
		}
	]
}