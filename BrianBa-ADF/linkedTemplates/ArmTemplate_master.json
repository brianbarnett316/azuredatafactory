					"Salesforce1_properties_typeProperties_username": {
						"value": "[parameters('Salesforce1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "BrianBa-ADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"NewCsvMemberFile_properties_Ingress Staging Area CSV to REST API_parameters_FileName": {
						"value": "[parameters('NewCsvMemberFile_properties_Ingress Staging Area CSV to REST API_parameters_FileName')]"
